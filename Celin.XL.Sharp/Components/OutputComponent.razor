
<MudStack Row="true" Spacing="0" Justify="Justify.SpaceBetween" AlignItems="AlignItems.Start">
    <MudText Typo="Typo.body2" Class="pl-4">
        <pre style="white-space: pre-wrap; overflow-x: auto;">
            @foreach (var o in Writer.Outputs)
            {
                switch (o.Type)
                {
                    case Service.WriterService.WriteTypes.Highlight:
                        <MudElement HtmlTag="span" Style="color: yellow;">@o.Text</MudElement>
                        break;
                    case Service.WriterService.WriteTypes.Normal:
                        <MudElement HtmlTag="span">@o.Text</MudElement>
                        break;
                    case Service.WriterService.WriteTypes.Error:
                        <MudElement HtmlTag="span" Style="color: red;">@o.Text</MudElement>
                        break;
                }
            }
        </pre>
    </MudText>
    <MudIconButton Icon="@Icons.Material.Filled.Clear" @onclick="Clear" />
</MudStack>