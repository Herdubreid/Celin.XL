@inherits BlazorStateComponent

<MudTextField
    @bind-Value="State.Command"
    Immediate="true"
    TextUpdateSuppression="false"
    Clearable="true"
    ErrorText="@State.CommandError"
    Error="HasError"
    id="commandPromptId"
    Style="font-family: Consolas; margin: 0 12px"
    T="string"
    FullWidth
    Variant="Variant.Text"
    Placeholder="Enter Command..."
    Lines="1"
    AutoGrow="true"
    AutoFocus="true"
    spellcheck="false"/>
